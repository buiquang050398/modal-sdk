{
  "name": "@brendan_bui/wallet-modal",
  "version": "1.0.5",
  "description": "Wallet Modal",
  "private": false,
  "type": "module",
  "main": "./dist/wallet-modal.umd.cjs",
  "module": "./dist/wallet-modal.js",
  "exports": {
    ".": {
      "import": "./dist/wallet-modal.js",
      "require": "./dist/wallet-modal.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "browser": {
    "./styles.css": "./dist/style.css"
  },
  "types": "./dist/wallet.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "ethers": "5.7.2",
    "pinia": "^2.1.6",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.11"
  }
}
